doctype html
html
    head
        include ../_modules/_head.jade
    body
        table#table(style='width: 100%;' data-options="url: 'http://127.0.0.1:10086/treegridData1', toolbar: '#toolbar-all'")
            thead(data-options="frozen: 'true'")
                tr
                    th(data-options="field: 'region', width: '10%'") Region
            thead
                tr
                    th(data-options="field: 'f1', width: '10%'") 9月16日（周五）
                    th(data-options="field: 'f2', width: '10%'") 9月17日（周六）
                    th(data-options="field: 'f3', width: '10%'") 9月18日（周日）
                    th(data-options="field: 'f4', width: '10%'") 9月19日（周一）
                    th(data-options="field: 'f5', width: '10%'") 9月20日（周二）
                    th(data-options="field: 'f6', width: '10%'") 9月21日（周三）
                    th(data-options="field: 'f7', width: '10%'") 9月22日（周四）
                    th(data-options="field: 'f8', width: '10%'") 9月23日（周五）
                    th(data-options="field: 'f9', width: '10%'") 9月24日（周六）
                    th(data-options="field: 'f10', width: '10%'") 9月25日（周日）
        // 工具栏
        div.display-none
            // 全部
            div#toolbar-all
                a.easyui-linkbutton#toolbar-all-look(data-options="iconCls:'icon-search', plain:true") 查看
    // js
    script(src='../../js/common/require.2.2.0.min.js')
    script(src='../../js/common/require.config.js')
    script.
        require(["common", "initTreegrid", "btnCtrlTable", "dbClickNewHref"], function(common, initTreegrid, btnCtrlTable, dbClickNewHref){
            var $table = $("#table");
            initTreegrid($table, "region");
            dbClickNewHref($table, "./个人办公-员工动静板-查看.html");
        });